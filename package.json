{
  "name": "Algorithm integration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josepot/alg-integration.git"
  },
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "format": "prettier --write README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "dev": "vite",
    "local": "VITE_ENV=local npm run dev",
    "build": "tsc --project tsconfig.build.json && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --env=./custom-test-env.js",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --env=./custom-test-env.js --watch",
    "lint": "prettier --check README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@polkadot/react-identicon": "^3.5.1",
    "@react-rxjs/core": "^0.10.4",
    "@react-rxjs/utils": "^0.9.5",
    "@tailwindcss/forms": "^0.5.3",
    "@unstoppablejs/client": "^0.2.1",
    "@unstoppablejs/substrate-bindings": "^0.0.1-rc.2",
    "@unstoppablejs/utils": "^1.1.0",
    "@unstoppablejs/ws-provider": "^0.0.1",
    "polkadot-theme": "^1.1.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rxjs": "^7.8.1",
    "scale-ts": "^0.2.11",
    "smoldot": "^1.0.11"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@json-rpc-tools/provider": "^1.7.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.11.26",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.3",
    "postcss": "^8.4.24",
    "postcss-focus-visible": "^8.0.2",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.1.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-modulepreload": "^2.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^28.0.3",
    "typescript": "^5.1.3",
    "unplugin-fonts": "^1.0.3",
    "vite": "^2.9.9"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "semi": false
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
}
